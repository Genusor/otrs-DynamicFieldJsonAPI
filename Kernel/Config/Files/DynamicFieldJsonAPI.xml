<?xml version="1.0" encoding="utf-8" ?>
<otrs_config version="1.0" init="Application">
    <ConfigItem Name="Frontend::Module###AdminDynamicFieldJsonAPI" Required="0" Valid="1">
        <Description Translatable="1">Frontend module registration for the agent interface.</Description>
        <Group>DynamicFieldJsonAPI</Group>
        <SubGroup>Frontend::Admin::ModuleRegistration</SubGroup>
        <Setting>
            <FrontendModuleReg>
                <Group>admin</Group>
                <Description>Admin</Description>
                <Title Translatable="1">Dynamic Fields Drop-down Backend GUI</Title>
                <Loader>
                    <CSS>Core.Agent.Admin.DynamicField.css</CSS>
                    <JavaScript>Core.Agent.Admin.DynamicField.js</JavaScript>
                    <JavaScript>Core.Agent.Admin.DynamicFieldJsonAPI.js</JavaScript>
                </Loader>
            </FrontendModuleReg>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="DynamicFields::Driver###JsonAPI" Required="0" Valid="1">
        <Description Translatable="1">DynamicField backend registration.</Description>
        <Group>DynamicFieldJsonAPI</Group>
        <SubGroup>DynamicFields::Driver::Registration</SubGroup>
        <Setting>
            <Hash>
                <Item Key="DisplayName" Translatable="1">JsonAPI</Item>
                <Item Key="Module">Kernel::System::DynamicField::Driver::JsonAPI</Item>
                <Item Key="ConfigDialog">AdminDynamicFieldJsonAPI</Item>
            </Hash>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="PublicFrontend::Module###PublicDeleteDFCache" Required="0" Valid="0">
        <Description Translatable="1">Frontend module registration to delete a dynamic field cache.</Description>
        <Group>DynamicFieldJsonAPI</Group>
        <SubGroup>Frontend::Public::ModuleRegistration</SubGroup>
        <Setting>
            <FrontendModuleReg>
                <Description>Delete DynamicField Cache</Description>
                <NavBarName></NavBarName>
                <Title>Delete DynamicField Cache</Title>
            </FrontendModuleReg>
        </Setting>
    </ConfigItem>
</otrs_config>
